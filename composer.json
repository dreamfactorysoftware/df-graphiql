{
    "name": "dreamfactory/df-graphiql",
    "description": "GraphiQL for DreamFactory",
    "keywords": ["dreamfactory", "graphql", "graphiql", "laravel"],
    "authors": [
        {
            "name": "Lee Hicks",
            "email": "leehicks@dreamfactory.com",
            "homepage": "http://dreamfactory.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "dreamfactory/df-core": "~0.13.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "webonyx/graphql-php": "~0.10.2"
    },
    "require-dev": {
        "orchestra/testbench": "3.1.*|3.2.*|3.3.*|3.4.*|3.5.*",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0|~5.0|~5.7|~6.0"
    },
    "autoload": {
        "psr-4": {
            "DreamFactory\\Core\\GraphiQL\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        },
        "laravel": {
            "providers": [
                "DreamFactory\\Core\\GraphiQL\\ServiceProvider"
            ]
        }
    }
}
