{
    "name": "dreamfactory/df-graphiql",
    "description": "GraphiQL for DreamFactory",
    "keywords": ["dreamfactory", "graphql", "graphiql", "laravel"],
    "authors": [
        {
            "name": "Lee Hicks",
            "email": "leehicks@dreamfactory.com",
        }
    ],
    "support":     {
        "email":  "dspsupport@dreamfactory.com",
        "source": "https://github.com/dreamfactorysoftware/df-graphiql",
        "issues": "https://github.com/dreamfactorysoftware/df-graphiql/issues",
        "wiki":   "https://wiki.dreamfactory.com"
    },
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "DreamFactory\\Core\\GraphiQL\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        },
        "laravel": {
            "providers": [
                "DreamFactory\\Core\\GraphiQL\\ServiceProvider"
            ]
        }
    }
}
